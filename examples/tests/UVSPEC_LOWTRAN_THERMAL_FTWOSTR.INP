# uvspec data files
data_files_path ../../data/
atmosphere_file ../AFGLUS.70KM
source thermal ../UVSPEC_LOWTRAN_THERMAL.TRANS

rte_solver ftwostr     # Radiative transfer equation solver
pseudospherical
wavelength_grid_file ../UVSPEC_LOWTRAN_THERMAL.TRANS

mol_abs_param LOWTRAN  # select LOWTRAN molecular absorption

output_process per_nm

quiet
