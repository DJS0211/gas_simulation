# basic thermal forward compuation with a normal atmosphere and correlated-k spectral integration
data_files_path ../../data/
atmosphere_file ../../data/atmmod/afglus.dat
source thermal
rte_solver montecarlo
mc_photons 1.e4
zout all_levels
albedo .1

output_process sum
mol_abs_param fu

wc_file 1D wc1D.dat
quiet
